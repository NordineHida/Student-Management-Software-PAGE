<Window x:Class="PAGE.Vue.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PAGE.Vue"
        ResizeMode="NoResize"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Height="500" Width="700">

    <!--Fond (Nordine)-->
    <Window.Background>
        <SolidColorBrush Color="#bfd7eb" />
    </Window.Background>

    <Window.Resources>
        <FontFamily x:Key="OpenSauceOneBlack">/Vue/Ressources/Fonts/#Open Sauce One Black</FontFamily>
    </Window.Resources>

    <!--Grille des éléments (Nordine)-->
    <Grid>
        <TextBlock x:Name="TitreBienvenue" Text="{DynamicResource TitreBienvenue}" FontSize="72" Margin="148,161,-76,255" FontFamily="{StaticResource OpenSauceOneBlack}" Foreground="#FF2B2B5A" />
        <Image Source="Ressources/Img/logoPage.png" Stretch="Uniform" VerticalAlignment="Top" HorizontalAlignment="Left" Height="228" Width="282" Margin="451,293,0,0" />
        <Image Source="Ressources/Img/logoIUT.png" Stretch="Uniform" VerticalAlignment="Top" HorizontalAlignment="Left" Height="169" Width="253" Margin="22,332,0,0" />
    </Grid>
</Window>
