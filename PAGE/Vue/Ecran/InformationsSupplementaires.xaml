                        <DataTrigger Binding="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType=Window}}" Value="600">
                            <Setter Property="FontSize" Value="16"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Button.Style>

        </Button>

        <!-- Bouton "Créer note" pour créer une note pour l'étudiant (Lucas) -->

        <Button FontSize="20" Foreground="#FFFFFF" Background="#FF3DA79D" FontFamily="{StaticResource OpenSauceOne}" Content="{DynamicResource BtnCreerNote}" x:Name="BoutonCreernote" Click="Creer_Click"  Margin="20,0,20,29" Grid.RowSpan="4" Grid.Row="9">
            <Button.Style>
                <Style TargetType="Button">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="Button">
                                <Border x:Name="border"  CornerRadius="10" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1">
                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#FF7dbfcf"/>
                                        <Setter Property="Foreground" Value="#000000"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType=Window}}" Value="300">
                            <Setter Property="FontSize" Value="12"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding ActualWidth, RelativeSource={RelativeSource AncestorType=Window}}" Value="600">
                            <Setter Property="FontSize" Value="16"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Button.Style>

        </Button>


        <StackPanel x:Name="contInfosComp"  Visibility="Collapsed" Orientation="Vertical" Grid.RowSpan="2" Grid.Row="19"  Grid.Column="1"  Grid.ColumnSpan="2">

            <!-- Date de naissance -->
            <WrapPanel>
                <TextBlock x:Name="txtDateNaissance1" FontFamily="{StaticResource OpenSauceOne}" MinWidth="50" FontSize="20" Text="{DynamicResource Birth}" Margin="0,20,0,0" />
                <DatePicker Grid.Column="3" FontFamily="{StaticResource OpenSauceOne}" FontSize="20" IsEnabled="False" x:Name="txtDateNaissance2" SelectedDateFormat="Short" Margin="30,20,0,0"></DatePicker>
            </WrapPanel>


            <!-- Adresse de résidence -->
            <WrapPanel>
                <TextBlock x:Name="txtAdresse1" FontFamily="{StaticResource OpenSauceOne}" FontSize="20" Text="{DynamicResource Adresse}" Margin="0,20,0,0"/>
                <TextBox Grid.Column="3" Width="425" FontFamily="{StaticResource OpenSauceOne}" IsReadOnly="True" x:Name="txtAdresse2" FontSize="15" Margin="30,20,0,0" MaxWidth="370" TextWrapping="NoWrap" BorderThickness="0"/>
            </WrapPanel>

            <!-- Téléphone fixe -->
            <WrapPanel>
                <TextBlock x:Name="txtTelFixe1" FontFamily="{StaticResource OpenSauceOne}" FontSize="20" Text="{DynamicResource TelFixe}" Margin="0,20,0,0" />
                <TextBox Grid.Column="3" Width="430" FontFamily="{StaticResource OpenSauceOne}" IsReadOnly="True" x:Name="txtTelFixe2" FontSize="15"  Margin="30,20,0,0"  BorderThickness="0"/>
            </WrapPanel>

            <!-- Téléphone portable -->
            <WrapPanel>
                <TextBlock x:Name="txtTelPortable1" FontFamily="{StaticResource OpenSauceOne}" FontSize="20" Text="{DynamicResource TelPort}" Margin="0,20,0,0"/>
                <TextBox Grid.Column="3" Width="425" FontFamily="{StaticResource OpenSauceOne}" IsReadOnly="True" x:Name="txtTelPortable2" FontSize="15"  Margin="30,20,0,0" MaxWidth="387" BorderThickness="0"/>
            </WrapPanel>

            <!-- Login réseau -->
            <WrapPanel>
                <TextBlock x:Name="txtLogin1" FontFamily="{StaticResource OpenSauceOne}" FontSize="20" Text="{DynamicResource LoginRes}" Margin="0,20,0,0" />
                <TextBox Grid.Column="3" Width="445" FontFamily="{StaticResource OpenSauceOne}" IsReadOnly="True" x:Name="txtLogin2" FontSize="15" Margin="30,20,0,0" BorderThickness="0" />
            </WrapPanel>

        </StackPanel>

        <ScrollViewer Margin="516,29,51,107"  Grid.RowSpan="20" Grid.Column="2" Grid.Row="4" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <WrapPanel Name="WrapPanelNote" Orientation="Vertical">

                <!-- Liste des note avec des NoteComponent (Nordine) générée dynamiquement ici -->
                <ItemsControl ItemsSource="{Binding NoteComponents}">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <!-- WrapPanel imbriqué avec ItemWidth défini -->
                            <WrapPanel Orientation="Horizontal" ItemWidth="200">
                            </WrapPanel>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>

                <!-- Gestionnaire d'événements pour le clic sur une note (Nordine) -->
                <WrapPanel.Resources>
                    <Style TargetType="local:NoteComponent">
                        <EventSetter Event="MouseDoubleClick" Handler="DoubleCliqueSurNote"/>
                    </Style>
                </WrapPanel.Resources>
            </WrapPanel>
        </ScrollViewer>



        <Label FontFamily="{StaticResource OpenSauceOne}" Grid.Column="2" FontSize="29" Content="Notes de l'étudiant : " HorizontalAlignment="Left" Margin="594,18,0,0" Grid.Row="1" VerticalAlignment="Top" Foreground="#2B2B5A" RenderTransformOrigin="2.452,0.87" Height="57" Width="278" Grid.RowSpan="3"/>

        <Line X1="50" Y1="10" X2="50" Y2="850" Stroke="Black" StrokeThickness="3" Margin="430,1,509,-61" Grid.RowSpan="20" Grid.Column="2" Grid.Row="1"/>
    </Grid>

</Window>
